	<queryString>
		<![CDATA[select p.civilite,p.nom as prof,m.type as typematiere,m.libelle as discipline,coef.valeur as coef,n1.valeur as note1,n2.valeur as note2,@valeurfinal:=(ifnull(n1.valeur,0)+ifnull(n2.valeur,0))/2 as notefinal,@valeurfinal*coef.valeur as total,e.nom as nomEleve,e.prenom,e.sexe,e.date_naissance,e.lieu_naissance,(concat(e.id,concat('E',date_format(e.created_date,'%Y')))) as matricule,e.photo,e.adresse,c.id as codeclasse,ei.redoublant
from coefficient coef join matiere m on (m.id=coef.matiere_id) join classe c on (c.serie_id=coef.serie_id) join classe_eleve ei on (ei.classe_id=c.id) join eleve e on (ei.eleve_id=e.id) left join cours on (cours.matiere_id=m.id and cours.classe_id=ei.classe_id and cours.annee_id=$P{code_annee})left join professeur p on p.id=cours.professeur_id  left join note n1 on (n1.classe_eleve_id=ei.id and n1.matiere_id=m.id  and n1.numero_sequence=($P{numtri}*2)-1)  left join note n2 on (n2.classe_eleve_id=ei.id and n2.matiere_id=m.id  and n2.numero_sequence=$P{numtri}*2)
where e.id =$P{code_eleve}  and ei.annee_id=$P{code_annee}]]>
	</queryString>
