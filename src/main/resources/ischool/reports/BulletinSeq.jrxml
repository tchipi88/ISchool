	<queryString>
		<![CDATA[select p.civilite,p.nom as prof,m.type as typematiere,m.libelle as discipline,n.valeur as note ,coef.valeur as coef,n.valeur*coef.valeur as total,(concat(e.id,concat('E',date_format(e.created_date,'%Y')))) as matricule,e.nom as nomEleve,e.prenom,e.sexe,e.adresse,e.date_naissance,e.lieu_naissance,c.id as codeclasse,e.photo,ei.redoublant
from coefficient coef join matiere m on (m.id=coef.matiere_id)  join classe c on (c.serie_id=coef.serie_id) join classe_eleve ei on (ei.classe_id=c.id) join eleve e on (ei.eleve_id=e.id) left join cours on (cours.matiere_id=m.id and cours.classe_id=ei.classe_id and cours.annee_id=$P{code_annee})left join professeur p on p.id=cours.professeur_id left join note n on (n.classe_eleve_id=ei.id and n.matiere_id=m.id  and (n.numero_sequence = $P{numseq}))  
where e.id =$P{code_eleve} and ei.annee_id= $P{code_annee} ;]]>
	</queryString>
