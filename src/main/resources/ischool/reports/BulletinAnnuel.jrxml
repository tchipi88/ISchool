	<queryString>
		<![CDATA[select p.civilite,p.nom AS prof, m.type as typematiere,m.libelle as discipline,coef.valeur as coef,n1.valeur as note1,n2.valeur as note2,
n3.valeur as note3,n4.valeur as note4,n5.valeur as note5,n6.valeur as note6,
@notefinal:=(ifnull(n1.valeur,0)+ifnull(n2.valeur,0)+ifnull(n3.valeur,0)+ifnull(n4.valeur,0)+ifnull(n5.valeur,0)+ifnull(n6.valeur,0))/6 as notefinal,
@notefinal*coef.valeur as total,e.nom,e.prenom,e.sexe,e.date_naissance,e.lieu_naissance,(concat(e.id,concat('E',date_format(e.created_date,'%Y')))) as matricule,e.photo,e.adresse,c.id as codeclasse,ei.redoublant
from coefficient coef join matiere m on (m.id=coef.matiere_id)  join classe c on (c.serie_id=coef.serie_id) join classe_eleve ei on (ei.classe_id=c.id) join eleve e on (ei.eleve_id=e.id)  
left join cours on (cours.matiere_id=m.id and cours.classe_id=ei.classe_id and cours.annee_id=$P{code_annee})left join professeur p on p.id=cours.professeur_id 
left join note n1 on (n1.classe_eleve_id=ei.id and n1.matiere_id=m.id  and n1.numero_sequence=1)  
left join note n2 on (n2.classe_eleve_id=ei.id and n2.matiere_id=m.id and n2.numero_sequence=2)
left join note n3 on (n2.classe_eleve_id=ei.id and n3.matiere_id=m.id and n2.numero_sequence=3)
left join note n4 on (n2.classe_eleve_id=ei.id and n4.matiere_id=m.id and n2.numero_sequence=4)
left join note n5 on (n2.classe_eleve_id=ei.id and n5.matiere_id=m.id and n2.numero_sequence=5)
left join note n6 on (n2.classe_eleve_id=ei.id and n6.matiere_id=m.id and n2.numero_sequence=6)

where e.id =$P{code_eleve}  and ei.annee_id=$P{code_annee} order by typematiere]]>
	</queryString>
